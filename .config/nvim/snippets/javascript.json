{
		"console.log": {
        "prefix": "cl",
        "body": "console.log(${0})"
    },
    "console.error": {
        "prefix": "ce",
        "body": "console.error(${0})"
    },
    "console.warn": {
        "prefix": "cw",
        "body": "console.warn(${0})"
    },
    "console.dir": {
        "prefix": "cd",
        "body": "console.dir('${1}:', ${1})"
    },
    "className": {
        "prefix": "cn",
        "body": "className={cn('${0}')}"
    },
    "function": {
        "prefix": "fc",
        "body": "function ${1} (${2}) {\n\t${0}\n}"
    },
    "arrow.function": {
        "prefix": "afc",
        "body": "const ${1} = (${2}) => {\n\t${0}\n}"
    },
    "return.jsx": {
        "prefix": "rjsx",
        "body": "return (\n\t<${1:tag}>\n\t\t${0}\n\t</$1>\n)"
    }
}
